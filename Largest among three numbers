#include<stdio.h>
int main(){
	int n1,n2,n3;
	printf("enter the three numbers:");
	scanf("%d\n %d\n %d",&n1,&n2,&n3);
	if(n1>=n2&&n1>=n3)
	printf("%d is greater value",n1);
	else if(n2>=n1&&n2>=n3)
	printf("%d is greater value",n2);
	else
	printf("%d is greater value",n3);
	return 0;
}

OUTPUT :
enter the three numbers:3
4
5
5 is greater value
